{
    "AWSEBDockerrunVersion": 2,
    "ContainerDefinitions": [
      {
          "name": "client",
          "image": "jamaldocker987/multi-client",
          "hostname": "client",
          "essential": false
      },
      {
        "name": "server",
        "image": "jamaldocker987/multi-server",
        "hostname": "api",
        "essential": false
    },
    {
        "name": "worker",
        "image": "jamaldocker987/multi-worker",
        "hostname": "worker",
        "essential": false
    },{
        "name": "nginx",
        "image": "jamaldocker987/multi-nginx",
        "essential": true,
        "portMappings": [
            {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["client", "server"]
    }
  ]   
}
